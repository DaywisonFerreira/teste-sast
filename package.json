{
    "name": "ifc.freight.api.orders",
    "version": "1.0.0",
    "description": "Freight API Orders",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Pedro Pereira <pedro.pereira.ext@infracommerce.com.br>",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git@gitlab.com:infralabs/cxaas/sourcecode/freight/ifc.freight.api.orders.git"
    },
    "keywords": [
        "nodejs",
        "nestjs",
        "microservice",
        "typescript",
        "freight",
        "api",
        "orders"
    ],
    "scripts": {
        "serve": "node dist/index.js",
        "start": "pm2 start --no-daemon dist/index.js -i max",
        "start:watch": "nodemon --legacy-watch --inspect=0.0.0.0:${DEBUG_PORT} dist",
        "build": "tsc",
        "build:watch": "tsc -w --skipLibCheck",
        "dev": "concurrently 'npm run build:watch''npm run start:watch'",
        "lint": "eslint src/ --ext .ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "test": "tsc && NODE_ENV=test jest --runInBand",
        "test:watch": "tsc --skipLibCheck && NODE_ENV=test jest --runInBand --watch"
    },
    "dependencies": {
        "@infralabs/infra-logger": "^1.4.1",
        "@types/express-session": "^1.17.4",
        "date-fns": "^2.23.0",
        "express-session": "^1.17.2",
        "ihub-framework-ts": "^1.9.7",
        "jsonwebtoken": "^8.5.1",
        "keycloak-connect": "^14.0.0",
        "module-alias": "^2.2.2",
        "mongodb": "3.6.3",
        "nodemailer": "^6.6.3",
        "typescript": "^4.2.4",
        "xlsx": "^0.17.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.24",
        "@types/mongodb": "3.6.19",
        "@types/nodemailer": "^6.4.4",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "concurrently": "^5.3.0",
        "eslint": "^7.18.0",
        "jest": "^27.0.4",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.2",
        "ts-jest": "^27.0.3",
        "ts-node": "^10.0.0"
    }
}
